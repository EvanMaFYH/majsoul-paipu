{
  "name": "majsoul-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_APP_INSTANCE=dev NODE_ENV=development nuxt",
    "build:dev": "cross-env NODE_APP_INSTANCE=dev NODE_ENV=production nuxt build",
    "build:prod": "cross-env NODE_APP_INSTANCE=prod NODE_ENV=production nuxt build",
    "generate:dev": "cross-env NODE_APP_INSTANCE=dev nuxt generate",
    "generate:prod": "cross-env NODE_APP_INSTANCE=prod nuxt generate",
    "launch:dev": "pm2 start ecosystem.config.js --env dev",
    "launch:prod": "pm2 start ecosystem.config.js --env prod",
    "launch": "nuxt start",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "config": "^3.3.6",
    "core-js": "^3.25.3",
    "core-util-is": "^1.0.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "element-ui": "^2.15.10",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "nuxt": "^2.15.8",
    "sass": "^1.32.13",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.1.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "sass-loader": "^10.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "vue-jest": "^3.0.4"
  }
}
